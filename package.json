{
  "name": "@express-worker/app",
  "version": "1.1.3",
  "description": "Express-style routing for Service Workers",
  "main": "dist/express-worker.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --config .prettierrc \"{src,tests}/*.{ts,js}\" --write",
    "build": "parcel build --no-scope-hoist & npm run browserify",
    "test": "karma start --single-run",
    "start": "karma start",
    "browserify": "browserify ./dist/express-worker.js --standalone ExpressWorker -o ./dist/express-worker.umd.js"
  },
  "express-worker": "dist/express-worker.js",
  "types": "dist/express-worker.d.ts",
  "targets": {
    "express-worker": {
      "source": "./src/index.ts",
      "distDir": "./dist"
    },
    "types": {
      "source": "./src/index.ts",
      "distDir": "./dist"
    }
  },
  "author": "Michael Puckett <michael@puckett.contact>",
  "license": "MIT",
  "dependencies": {
    "path-to-regexp": "^6.2.1"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.10.3",
    "@parcel/transformer-typescript-tsc": "^2.10.3",
    "@parcel/transformer-typescript-types": "^2.10.3",
    "@types/jasmine": "^5.1.4",
    "browserify": "^17.0.0",
    "jasmine-core": "^5.1.1",
    "karma": "^6.4.2",
    "parcel": "^2.10.3",
    "prettier": "^3.1.1",
    "requirejs": "^2.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  }
}
