{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "jsx": "react-jsx",
    "noLib": false,
    "isolatedModules": true,
    "lib": ["ESNext", "webworker", "DOM.Iterable"],
    "target": "ESNext",
    "outDir": "./"
  },
  "exclude": ["./node_modules"],
  "include": ["src/**/*"]
}
